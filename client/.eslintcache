Count":0,"fixableWarningCount":0},{"filePath":"33","messages":"34","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"35"},"/Users/sarahrourke/Desktop/praha/praha-store/client/src/App.js",["36","37"],"/Users/sarahrourke/Desktop/praha/praha-store/client/src/index.js",[],"/Users/sarahrourke/Desktop/praha/praha-store/client/src/components/Items.js",[],"/Users/sarahrourke/Desktop/praha/praha-store/client/src/components/Header.js",["38","39","40"],"import React from 'react';\nimport { Container, Navbar, Nav, Form, FormControl, Button } from 'react-bootstrap';\nimport Praha_Logo from '../Praha_Logo.svg';\nimport './Header.css';\n\nconst Header = () => {\n\n    return ( \n        <Container fluid className=\"header\">\n            <Navbar expand=\"lg\">\n                <Navbar.Brand href=\"/\">\n                    <img src={Praha_Logo} className=\"praha-logo\" alt=\"praha\"/>\n                </Navbar.Brand>\n                {/* <Form inline className=\"searchbar\">\n                    <FormControl type=\"text\" placeholder=\"Search\" className=\"mr-sm-2\" />\n                    <Button variant=\"outline-primary\">Search</Button>\n                </Form> */}\n                <Nav>\n                    <Nav.Link href=\"/\">Home</Nav.Link>\n                    <Nav.Link href=\"/items\">Items</Nav.Link>\n                    <Nav.Link href=\"/create\">Add New Items</Nav.Link>\n                </Nav>\n\n                \n            </Navbar>\n        </Container>\n    )\n}\n\nexport default Header;","/Users/sarahrourke/Desktop/praha/praha-store/client/src/components/Item.js",[],"/Users/sarahrourke/Desktop/praha/praha-store/client/src/components/Home.js",[],"/Users/sarahrourke/Desktop/praha/praha-store/client/src/components/AddItemForm.js",[],"/Users/sarahrourke/Desktop/praha/praha-store/client/src/components/EditItemForm.js",["41"],"import React, { useState, useEffect } from 'react';\nimport axios from 'axios';\nimport { Container, Form, Button } from 'react-bootstrap';\n\nconst EditItemForm = (props) => {\n    \n\n    const initialFormState = {\n        name: '',\n        price: '',\n        category: '',\n        image: {}\n        }\n    //initialFormState is an empty form, calls get method\n    const [item, setItem] = useState(initialFormState);\n    \n    \n    // onChange, tracks the key strong change for input\n    const onChange = (e) => {\n\n        setItem(() => {\n            if (e.target.name === \"image\")\n                return ({ ...item, image : e.target.files[0] })\n            else    \n                return ({ ...item,\n                \n                    [e.target.name] : e.target.value \n                });\n             });\n            };\n    \n    \n    // onSubmit -> edit item name, category, price, not image \n    const onSubmit = (e) => {\n\n        e.preventDefault();\n\n        axios.put(`/api/v1/items/${props.match.params.id}`, item)\n        .then(resp => {\n            setItem(resp.data)\n        })\n        .catch(error => console.log(error, error),\n        //redirects the page to the item 'show' page after successful update\n        props.history.push(`/items/${props.match.params.id}`))\n    }\n    \n\n    \n\n\n\n\n\n    return (\n\n        <Container fluid className=\"main\">\n            \n            \n            <form onSubmit={onSubmit}>\n                <Form.Group controlid=\"editItemForm\">\n                    <Form.Label>Edit This Item</Form.Label>\n                        <h3>{console.log(item)}</h3>\n                        <p>New Name: </p>\n                        <Form.Control   \n                            as=\"input\"\n                            type=\"text\"\n                            value={item.name}\n                            placeholder=\"Update Item Name?\"\n                            name=\"name\"\n                            onChange={onChange} \n                        />\n\n                        <p>New Price: </p>\n                        <Form.Control   \n                            as=\"input\"\n                            type=\"text\"\n                            value={item.price}\n                            placeholder=\"Update Item Price?\"\n                            name=\"price\"\n                            onChange={onChange} \n                        />\n\n                        <select\n                        name=\"category\"\n                        category={item.value}\n                        onChange={onChange}>\n                            <option\n                            value=\"shelving\">Shelving</option>\n                            <option\n                            value=\"lighting\">Lighting</option>\n                            <option\n                            value=\"wall decor\">Wall Decor</option>\n                            <option\n                            value=\"tables\">Tables</option>\n                            <option\n                            value=\"seating\">Seating</option>\n                            <option\n                            value=\"kitchen\">Kitchen</option>\n                        </select>\n\n                        <p>New Item Image: </p>\n                        <Form.File\n                            id=\"itemImage\"\n                            type=\"file\"\n                            label=\"Item Photo\"\n                            name=\"image\"\n                            onChange={onChange} \n                        />\n\n                    <Button type=\"submit\" variant=\"outline-info\">Edit</Button>\n\n                </Form.Group>\n            </form>\n        \n        </Container>\n    )\n\n}\n\nexport default EditItemForm;",{"ruleId":"42","severity":1,"message":"43","line":1,"column":8,"nodeType":"44","messageId":"45","endLine":1,"endColumn":18},{"ruleId":"42","severity":1,"message":"46","line":3,"column":27,"nodeType":"44","messageId":"45","endLine":3,"endColumn":33},{"ruleId":"